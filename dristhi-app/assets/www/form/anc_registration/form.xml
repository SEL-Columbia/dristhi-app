                <input autocomplete="off" type="radio" name="/ANC_Registration_EngKan/h_blood_group" value="ab_negative"
                       data-relevant=" /ANC_Registration_EngKan/w_rh_negative  = 'yes'" data-type-xml="select1"/>
                <span lang="" class=" active">AB-</span>
