            <label class="clearfix ">
                <input autocomplete="off" type="radio" name="/ANC_Registration_EngKan/is_high_risk" value="no"
                       data-type-xml="select1"/>
                <span lang="" class=" active">No &#xC87;&#xCB2;&#xCCD;&#xCB2;</span>
            </label>
        </fieldset>
