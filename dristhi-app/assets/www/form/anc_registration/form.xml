    <fieldset class="restoring-sanity-to-legends ">
        <fieldset>
            <legend>
                <span lang="" class=" active">Is tobacco used in the household? &#xCAE;&#xCA8;&#xCC6;&#xCAF;&#xCB2;&#xCCD;&#xCB2;&#xCBF;
                    &#xCA4;&#xC82;&#xCAC;&#xCBE;&#xC95;&#xCC1; &#xCAC;&#xCB3;&#xCB8;&#xCB2;&#xCBE;&#xC97;&#xCC1;&#xCA4;&#xCCD;&#xCA4;&#xCBF;&#xCA6;&#xCC6;&#xCAF;&#xCC7;?
                </span>
            </legend>
            <label class="clearfix ">
                <input autocomplete="off" type="radio" name="/ANC_Registration_EngKan/tobacco_use" value="yes"
                       data-type-xml="select1"/>
                <span lang="" class=" active">Yes &#xCB9;&#xCCC;&#xCA6;&#xCC1;</span>
            </label>
            <label class="clearfix ">
                <input autocomplete="off" type="radio" name="/ANC_Registration_EngKan/tobacco_use" value="no"
                       data-type-xml="select1"/>
                <span lang="" class=" active">No &#xC87;&#xCB2;&#xCCD;&#xCB2;</span>
            </label>
        </fieldset>
    </fieldset>
    <fieldset class="restoring-sanity-to-legends jr-branch pre-init ">
        <fieldset>
            <legend>
                <span lang="" class=" active">Who uses tobacco in household? &#xCAE;&#xCA8;&#xCC6;&#xCAF;&#xCB2;&#xCCD;&#xCB2;&#xCBF;
                    &#xCAF;&#xCBE;&#xCB0;&#xCC1; &#xCA4;&#xC82;&#xCAC;&#xCBE;&#xC95;&#xCC1; &#xCAC;&#xCB3;&#xCB8;&#xCC1;&#xCA4;&#xCCD;&#xCA4;&#xCC0;&#xCA6;&#xCCD;&#xCA6;&#xCBE;&#xCB0;&#xCC6;?
                </span>
            </legend>
            <label class="clearfix ">
                <input autocomplete="off" type="checkbox" name="/ANC_Registration_EngKan/tobacco_user"
                       value="woman_uses_tobacco" data-relevant=" /ANC_Registration_EngKan/tobacco_use  = 'yes'"
                       data-type-xml="select"/>
                <span lang="" class=" active">Pregnant woman &#xC97;&#xCB0;&#xCCD;&#xCAD;&#xCBF;&#xCA3;&#xCBF; &#xCAE;&#xCB9;&#xCBF;&#xCB3;&#xCC6;</span>
            </label>
            <label class="clearfix ">
                <input autocomplete="off" type="checkbox" name="/ANC_Registration_EngKan/tobacco_user"
                       value="husband_uses_tobacco" data-relevant=" /ANC_Registration_EngKan/tobacco_use  = 'yes'"
                       data-type-xml="select"/>
                <span lang="" class=" active">Husband &#xC97;&#xC82;&#xCA1;</span>
            </label>
            <label class="clearfix ">
                <input autocomplete="off" type="checkbox" name="/ANC_Registration_EngKan/tobacco_user"
                       value="others_use_tobacco" data-relevant=" /ANC_Registration_EngKan/tobacco_use  = 'yes'"
                       data-type-xml="select"/>
                <span lang="" class=" active">Others &#xC87;&#xCA4;&#xCB0;&#xCC6;</span>
            </label>
        </fieldset>
