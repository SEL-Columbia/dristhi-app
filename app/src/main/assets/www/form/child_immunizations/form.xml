<form autocomplete="off" class="or clearfix" id="Child_Immunizations_EngKan"
    novalidate="novalidate">
    <!--This form was created by transforming a OpenRosa-flavored (X)Form using an XSLT sheet created by Enketo LLC.-->
    <section class="form-logo"></section>
    <h3 id="form-title">Child Immunization</h3>

    <label class="question non-select ">
        <span class="question-label active" lang="">Mother Name</span>
        <input name="/Child_Immunizations_EngKan/mother_name" autocomplete="off"
            data-type-xml="string" disabled="disabled" type="text" />
    </label>

    <label class="question non-select ">

        <input name="/Child_Immunizations_EngKan/village" autocomplete="off" data-type-xml="string"
            disabled="disabled" type="hidden" />
    </label>


    <fieldset class="question ">
        <fieldset>
            <legend>
                <span class="question-label active" lang="">Immunizations provided to child
                </span>
                <span class="required">*</span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input name="/Child_Immunizations_EngKan/entity_immunizations_received"
                        autocomplete="off" data-type-xml="select" required="required"
                        type="checkbox" value="bcg" />
                    <span class="option-label active" lang="">BCG</span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/entity_immunizations_received"
                        autocomplete="off" data-type-xml="select" required="required"
                        type="checkbox" value="opv_0" />
                    <span class="option-label active" lang="">OPV 0</span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/entity_immunizations_received"
                        autocomplete="off" data-type-xml="select" required="required"
                        type="checkbox" value="hepb_0" />
                    <span class="option-label active" lang="">Hep B 0
                    </span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/entity_immunizations_received"
                        autocomplete="off" data-type-xml="select" required="required"
                        type="checkbox" value="opv_1" />
                    <span class="option-label active" lang="">OPV 1</span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/entity_immunizations_received"
                        autocomplete="off" data-type-xml="select" required="required"
                        type="checkbox" value="pentavalent_1" />
                    <span class="option-label active" lang="">Pentavalent 1
                    </span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/entity_immunizations_received"
                        autocomplete="off" data-type-xml="select" required="required"
                        type="checkbox" value="opv_2" />
                    <span class="option-label active" lang="">OPV 2</span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/entity_immunizations_received"
                        autocomplete="off" data-type-xml="select" required="required"
                        type="checkbox" value="pentavalent_2" />
                    <span class="option-label active" lang="">Pentavalent 2
                    </span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/entity_immunizations_received"
                        autocomplete="off" data-type-xml="select" required="required"
                        type="checkbox" value="opv_3" />
                    <span class="option-label active" lang="">OPV 3</span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/entity_immunizations_received"
                        autocomplete="off" data-type-xml="select" required="required"
                        type="checkbox" value="pentavalent_3" />
                    <span class="option-label active" lang="">Pentavalent 3
                    </span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/entity_immunizations_received"
                        autocomplete="off" data-type-xml="select" required="required"
                        type="checkbox" value="measles" />
                    <span class="option-label active" lang="">Measles 1</span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/entity_immunizations_received"
                        autocomplete="off" data-type-xml="select" required="required"
                        type="checkbox" value="je" />
                    <span class="option-label active" lang="">JE 1</span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/entity_immunizations_received"
                        autocomplete="off" data-type-xml="select" required="required"
                        type="checkbox" value="mmr" />
                    <span class="option-label active" lang="">MMR</span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/entity_immunizations_received"
                        autocomplete="off" data-type-xml="select" required="required"
                        type="checkbox" value="dptbooster_1" />
                    <span class="option-label active" lang="">DPT Booster 1
                    </span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/entity_immunizations_received"
                        autocomplete="off" data-type-xml="select" required="required"
                        type="checkbox" value="opvbooster" />
                    <span class="option-label active" lang="">OPV Booster</span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/entity_immunizations_received"
                        autocomplete="off" data-type-xml="select" required="required"
                        type="checkbox" value="measlesbooster" />
                    <span class="option-label active" lang="">Measles 2</span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/entity_immunizations_received"
                        autocomplete="off" data-type-xml="select" required="required"
                        type="checkbox" value="je_2" />
                    <span class="option-label active" lang="">JE 2</span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/entity_immunizations_received"
                        autocomplete="off" data-type-xml="select" required="required"
                        type="checkbox" value="dptbooster_2" />
                    <span class="option-label active" lang="">DPT Booster 2 (5 years)
                    </span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <fieldset class="question ">
        <fieldset>
            <legend>
                <span class="question-label active" lang="">Place of immunization?
                </span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input name="/Child_Immunizations_EngKan/immunization_place" autocomplete="off"
                        data-name="/Child_Immunizations_EngKan/immunization_place"
                        data-type-xml="select1" type="radio" value="sub_center" />
                    <span class="option-label active" lang="">My Sub Center</span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/immunization_place" autocomplete="off"
                        data-name="/Child_Immunizations_EngKan/immunization_place"
                        data-type-xml="select1" type="radio" value="phc" />
                    <span class="option-label active" lang="">My PHC</span>
                </label>
                <label class="">
                    <input name="/Child_Immunizations_EngKan/immunization_place" autocomplete="off"
                        data-name="/Child_Immunizations_EngKan/immunization_place"
                        data-type-xml="select1" type="radio" value="elsewhere" />
                    <span class="option-label active" lang="">Elsewhere
                    </span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <label class="question non-select ">
        <span class="question-label active" lang="">Immunization date?
        </span>
        <span class="required">*</span>
        <input name="/Child_Immunizations_EngKan/immunization_date" autocomplete="off"
            data-constraint="if(selected( /Child_Immunizations_EngKan/entity_immunizations_received , 'bcg') or selected( /Child_Immunizations_EngKan/entity_immunizations_received , 'opv_0') or selected( /Child_Immunizations_EngKan/entity_immunizations_received , 'hepb_0'), .&gt;=  /Child_Immunizations_EngKan/case_child_birth_date  and .&lt;= /Child_Immunizations_EngKan/today , if(selected( /Child_Immunizations_EngKan/entity_immunizations_received , 'opv_1') or selected( /Child_Immunizations_EngKan/entity_immunizations_received , 'pentavalent_1'), .&gt;=  /Child_Immunizations_EngKan/three_weeks  and .&lt;= /Child_Immunizations_EngKan/today , if(selected( /Child_Immunizations_EngKan/entity_immunizations_received , 'opv_2') or selected( /Child_Immunizations_EngKan/entity_immunizations_received , 'pentavalent_2'), .&gt;= /Child_Immunizations_EngKan/seven_weeks  and .&lt;= /Child_Immunizations_EngKan/today , if(selected( /Child_Immunizations_EngKan/entity_immunizations_received , 'opv_3') or selected( /Child_Immunizations_EngKan/entity_immunizations_received , 'pentavalent_3'), .&gt;= /Child_Immunizations_EngKan/eleven_weeks  and .&lt;= /Child_Immunizations_EngKan/today , if(selected( /Child_Immunizations_EngKan/entity_immunizations_received , 'measles'), .&gt;= /Child_Immunizations_EngKan/eight_months  and .&lt;= /Child_Immunizations_EngKan/today , if(selected( /Child_Immunizations_EngKan/entity_immunizations_received , 'mmr') or selected( /Child_Immunizations_EngKan/entity_immunizations_received , 'dptbooster_1') or selected( /Child_Immunizations_EngKan/entity_immunizations_received , 'opvbooster') or selected( /Child_Immunizations_EngKan/entity_immunizations_received , 'measlesbooster'), .&gt;=  /Child_Immunizations_EngKan/sixteen_months  and .&lt;= /Child_Immunizations_EngKan/today , if(selected( /Child_Immunizations_EngKan/entity_immunizations_received , 'dptbooster_2'), .&gt;= /Child_Immunizations_EngKan/four_years  and .&lt;= /Child_Immunizations_EngKan/today , .&lt;= /Child_Immunizations_EngKan/today )))))))"
            data-type-xml="date" required="required" type="date" />
    </label>

    <fieldset style="display:none;" id="or-preload-items">
        <label class="calculation non-select ">
            <input name="/Child_Immunizations_EngKan/today" autocomplete="off" data-preload="date"
                data-preload-params="today" data-type-xml="date" type="hidden" />
        </label>
    </fieldset>
    <fieldset style="display:none;" id="or-calculated-items">
        <label class="calculation non-select ">
            <input name="/Child_Immunizations_EngKan/three_weeks" autocomplete="off"
                data-calculate=" /Child_Immunizations_EngKan/case_child_birth_date  + 21"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Immunizations_EngKan/seven_weeks" autocomplete="off"
                data-calculate=" /Child_Immunizations_EngKan/case_child_birth_date  + 49"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Immunizations_EngKan/eleven_weeks" autocomplete="off"
                data-calculate=" /Child_Immunizations_EngKan/case_child_birth_date  + 77"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Immunizations_EngKan/eight_months" autocomplete="off"
                data-calculate=" /Child_Immunizations_EngKan/case_child_birth_date  + 243"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Immunizations_EngKan/sixteen_months" autocomplete="off"
                data-calculate=" /Child_Immunizations_EngKan/case_child_birth_date  + 486"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Immunizations_EngKan/four_years" autocomplete="off"
                data-calculate=" /Child_Immunizations_EngKan/case_child_birth_date  + 1461"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Immunizations_EngKan/immunizations_given" autocomplete="off"
                data-calculate="concat( /Child_Immunizations_EngKan/entity_immunizations_given , ' ',  /Child_Immunizations_EngKan/entity_immunizations_received )"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Immunizations_EngKan/meta/instanceID" autocomplete="off"
                data-calculate="concat('uuid:', uuid())" data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Immunizations_EngKan/formhub/uuid" autocomplete="off"
                data-calculate="'517d55ce8f854b1191f1148b41eb8879'" data-type-xml="string"
                type="hidden" />
        </label>
    </fieldset>
</form>
