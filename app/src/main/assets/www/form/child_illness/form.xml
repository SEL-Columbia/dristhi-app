        <!--</fieldset>-->

        <!--</fieldset>-->

    </section>
    <!--end of group -->
    <section name="/Child_Sick_Visit_EngKan/report_child_disease_group"
        class="or-group-data or-branch pre-init "
        data-relevant=" /Child_Sick_Visit_EngKan/sick_visit_option  = 'report_child_disease'">
        <fieldset class="question ">
            <fieldset>
                <legend>
                    <span class="question-label active" lang="">Select child disease to report
                    </span>
                    <span class="required">*</span>
                </legend>
                <div class="option-wrapper">
                    <label class="">
                        <input
                            name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease"
                            autocomplete="off"
                            data-name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease"
                            data-type-xml="select1" required="required" type="radio"
                            value="measles" />
                        <span class="option-label active" lang="">Measles</span>
                    </label>
                    <label class="">
                        <input
                            name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease"
                            autocomplete="off"
                            data-name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease"
                            data-type-xml="select1" required="required" type="radio"
                            value="diarrhea_dehydration" />
                        <span class="option-label active" lang="">Diarrhea and Dehydration</span>
                    </label>
                    <label class="">
                        <input
                            name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease"
                            autocomplete="off"
                            data-name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease"
                            data-type-xml="select1" required="required" type="radio"
                            value="malaria" />
                        <span class="option-label active" lang="">Malaria</span>
                    </label>
                    <label class="">
                        <input
                            name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease"
                            autocomplete="off"
                            data-name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease"
                            data-type-xml="select1" required="required" type="radio" value="ari" />
                        <span class="option-label active" lang="">Acute Respiratory Infection (ARI)
                        </span>
                    </label>
                    <label class="">
                        <input
                            name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease"
                            autocomplete="off"
                            data-name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease"
                            data-type-xml="select1" required="required" type="radio" value="sam" />
                        <span class="option-label active" lang="">Severe Acute Malnutrition</span>
                    </label>
                    <label class="">
                        <input
                            name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease"
                            autocomplete="off"
                            data-name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease"
                            data-type-xml="select1" required="required" type="radio"
                            value="others" />
                        <span class="option-label active" lang="">Others</span>
                    </label>
                </div>
            </fieldset>
        </fieldset>
        <label class="question or-branch pre-init non-select ">
            <span class="question-label active" lang="">Others</span>
            <input
                name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease_other"
                autocomplete="off"
                data-relevant=" /Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease  = 'others'"
                data-type-xml="string" type="text" />
        </label>
        <label class="question non-select ">
            <span class="question-label active" lang="">Reporting date?
            </span>
            <span class="required">*</span>
            <input
                name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease_date"
                autocomplete="off"
                data-constraint=".&gt;=  /Child_Sick_Visit_EngKan/child_sickness_management_group/case_child_birth_date  and .&lt;= /Child_Sick_Visit_EngKan/today "
                data-type-xml="date" required="required" type="date" />
        </label>
        <fieldset class="question ">
            <fieldset>
                <legend>
                    <span class="question-label active" lang="">Reporting place?
                    </span>
                </legend>
                <div class="option-wrapper">
                    <label class="">
                        <input
                            name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease_place"
                            autocomplete="off"
                            data-name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease_place"
                            data-type-xml="select1" type="radio" value="sub_center" />
                        <span class="option-label active" lang="">My Sub Center</span>
                    </label>
                    <label class="">
                        <input
                            name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease_place"
                            autocomplete="off"
                            data-name="/Child_Sick_Visit_EngKan/report_child_disease_group/report_child_disease_place"
                            data-type-xml="select1" type="radio" value="phc" />
