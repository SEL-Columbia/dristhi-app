    <fieldset class="question ">
        <fieldset>
            <legend>
                <span class="question-label active" lang="">Do you want to Consult Doctor?
                </span>
                <span class="required">*</span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input name="/Child_Sick_Visit_EngKan/is_consult_doctor"
                        data-name="/Child_Sick_Visit_EngKan/is_consult_doctor"
                        data-type-xml="select1" required="required" type="radio" value="yes" />
                    <span class="option-label active" lang="">Yes</span>
                </label>
                <label class="">
                    <input name="/Child_Sick_Visit_EngKan/is_consult_doctor"
                        data-name="/Child_Sick_Visit_EngKan/is_consult_doctor"
                        data-type-xml="select1" required="required" type="radio" value="no" />
                    <span class="option-label active" lang="">No</span>
                </label>
            </div>
        </fieldset>

    </fieldset>

    <label class="question or-branch pre-init non-select ">
        <span class="question-label active" lang="">Reason ?
        </span>
        <span class="required">*</span>
        <input name="/Child_Sick_Visit_EngKan/is_consult_doctor_no_reason" autocomplete="off"
            data-relevant=" /Child_Sick_Visit_EngKan/reason_for_no  = 'no'" data-type-xml="string"
            required="required" type="text" />
    </label>


    <fieldset style="display:none;" id="or-preload-items">
        <label class="calculation non-select ">
            <input name="/Child_Sick_Visit_EngKan/today" autocomplete="off" data-preload="date"
                data-preload-params="today" data-type-xml="date" type="hidden" />
        </label>
    </fieldset>
    <fieldset style="display:none;" id="or-calculated-items">


        <label class="calculation non-select ">
            <input name="/Child_Sick_Visit_EngKan/reason_for_no" autocomplete="off"
                data-calculate=" /Child_Sick_Visit_EngKan/is_consult_doctor" data-type-xml="string"
                type="hidden" />
        </label>


        <label class="calculation non-select ">
            <input name="/Child_Sick_Visit_EngKan/child_sickness_management_group/child_age"
                autocomplete="off"
                data-calculate="int(( /Child_Sick_Visit_EngKan/child_sickness_management_group/sick_visit_date  -  /Child_Sick_Visit_EngKan/child_sickness_management_group/case_child_birth_date ) div 30.42)"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input
                name="/Child_Sick_Visit_EngKan/child_sickness_management_group/cough_group/fast_breathing"
                autocomplete="off"
                data-calculate="if( /Child_Sick_Visit_EngKan/child_sickness_management_group/child_age  &lt; 12 and  /Child_Sick_Visit_EngKan/child_sickness_management_group/cough_group/breaths_per_minute  &gt;= 50, 'yes', if( /Child_Sick_Visit_EngKan/child_sickness_management_group/child_age  &gt;=12 and  /Child_Sick_Visit_EngKan/child_sickness_management_group/cough_group/breaths_per_minute  &gt;= 40, 'yes', ''))"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Sick_Visit_EngKan/child_sickness_management_group/ds_cough"
                autocomplete="off"
                data-calculate="if( /Child_Sick_Visit_EngKan/child_sickness_management_group/cough_group/days_of_cough  &gt;= 21, 'Cough_for_21_days_or_more', '')"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Sick_Visit_EngKan/child_sickness_management_group/ds_diarrhea"
                autocomplete="off"
                data-calculate="if( /Child_Sick_Visit_EngKan/child_sickness_management_group/diarrhea_group/days_of_diarrhea  &gt;= 14, 'Diarrhea_for_14_days_or_more', '')"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Sick_Visit_EngKan/child_sickness_management_group/ds_blood_in_stool"
                autocomplete="off"
                data-calculate="if( /Child_Sick_Visit_EngKan/child_sickness_management_group/diarrhea_group/blood_in_stool  = 'yes', 'Blood_in_stool', '')"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Sick_Visit_EngKan/child_sickness_management_group/ds_fever"
                autocomplete="off"
                data-calculate="if( /Child_Sick_Visit_EngKan/child_sickness_management_group/fever_group/days_of_fever  &gt;= 7, 'Fever_for_7_days_or_more', '')"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Sick_Visit_EngKan/child_sickness_management_group/ds_convulsions"
                autocomplete="off"
                data-calculate="if(selected( /Child_Sick_Visit_EngKan/child_sickness_management_group/child_signs , 'convulsions'), 'Convulsions', '')"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input
                name="/Child_Sick_Visit_EngKan/child_sickness_management_group/ds_not_able_drink_feed"
                autocomplete="off"
                data-calculate="if( /Child_Sick_Visit_EngKan/child_sickness_management_group/drink_feed_nothing  = 'yes', 'Not_able_to_drink_or_feed_anything', '')"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input
                name="/Child_Sick_Visit_EngKan/child_sickness_management_group/ds_vomiting_everything"
                autocomplete="off"
                data-calculate="if( /Child_Sick_Visit_EngKan/child_sickness_management_group/vomiting_everything  = 'yes', 'Vomiting_everything', '')"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input
                name="/Child_Sick_Visit_EngKan/child_sickness_management_group/ds_chest_indrawing"
                autocomplete="off"
                data-calculate="if(selected( /Child_Sick_Visit_EngKan/child_sickness_management_group/child_signs , 'chest_indrawing'), 'Chest_indrawing', '')"
                data-type-xml="string" type="hidden" />
        </label>

        <label class="calculation non-select ">
            <input name="/Child_Sick_Visit_EngKan/temperature" data-calculate="0"
                data-type-xml="string" type="hidden" />
        </label>

        <label class="calculation non-select ">
            <input
                name="/Child_Sick_Visit_EngKan/child_sickness_management_group/ds_sleepy_unconscious"
                autocomplete="off"
                data-calculate="if(selected( /Child_Sick_Visit_EngKan/child_sickness_management_group/child_signs , 'sleepy_unconscious'), 'Unusually_sleepy_or_unconscious', '')"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Sick_Visit_EngKan/child_sickness_management_group/ds_swelling_feet"
                autocomplete="off"
                data-calculate="if(selected( /Child_Sick_Visit_EngKan/child_sickness_management_group/child_signs , 'swelling_both_feet'), 'Swelling_of_both_feet', '')"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input
                name="/Child_Sick_Visit_EngKan/child_sickness_management_group/immediate_referral"
                autocomplete="off"
                data-calculate="if( /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_cough  != '' or  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_diarrhea  != '' or  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_blood_in_stool  != '' or  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_fever  != '' or  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_convulsions  != '' or  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_not_able_drink_feed  != '' or  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_vomiting_everything  != '' or  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_chest_indrawing  != '' or  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_sleepy_unconscious  != '' or  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_swelling_feet  != '', 'yes', '')"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input
                name="/Child_Sick_Visit_EngKan/child_sickness_management_group/immediate_referral_reason"
                autocomplete="off"
                data-calculate="concat( /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_cough , ' ',  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_diarrhea , ' ',  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_blood_in_stool , ' ',  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_fever , ' ',  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_convulsions , ' ',  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_not_able_drink_feed , ' ',  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_vomiting_everything , ' ',  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_chest_indrawing , ' ',  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_sleepy_unconscious , ' ',  /Child_Sick_Visit_EngKan/child_sickness_management_group/ds_swelling_feet )"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Sick_Visit_EngKan/anm_poc" data-calculate="" data-type-xml="string"
                type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Sick_Visit_EngKan/meta/instanceID" autocomplete="off"
                data-calculate="concat('uuid:', uuid())" data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Sick_Visit_EngKan/formhub/uuid" autocomplete="off"
                data-calculate="'407004fdbaf34751b2983f0d63b2f2a6'" data-type-xml="string"
                type="hidden" />
        </label>
    </fieldset>
