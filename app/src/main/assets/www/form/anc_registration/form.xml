<form autocomplete="off" class="or clearfix" id="ANC_Registration_EngKan" novalidate="novalidate">
    <!--This form was created by transforming a OpenRosa-flavored (X)Form using an XSLT sheet created by Enketo LLC.-->
    <section class="form-logo"></section>
    <h3 id="form-title">ANC Registration</h3>

    <label class="question non-select ">
        <span class="question-label active" lang="">EC Number:</span>
        <input name="/ANC_Registration_EngKan/ec_num" autocomplete="off" data-type-xml="int"
            disabled="disabled" type="text" />
    </label>

    <label class="question non-select ">

        <input name="/ANC_Registration_EngKan/village" autocomplete="off" data-type-xml="string"
            disabled="disabled" type="hidden" />
    </label>


    <label class="question non-select ">
        <span class="question-label active" lang="">Woman Name:</span>
        <input name="/ANC_Registration_EngKan/woman_name" autocomplete="off" data-type-xml="string"
            disabled="disabled" type="text" />
    </label>

    <label class="question non-select ">
        <span class="question-label active" lang="">Husband Name:</span>
        <input name="/ANC_Registration_EngKan/husband_name" autocomplete="off"
            data-type-xml="string" disabled="disabled" type="text" />
    </label>

    <label class="question non-select ">
        <span class="question-label active" lang="">Registration date:
        </span>
        <span class="required">*</span>
        <input name="/ANC_Registration_EngKan/reg_date" autocomplete="off"
            data-constraint=".&lt;= /ANC_Registration_EngKan/today " data-type-xml="date"
            required="required" type="date" />
    </label>
    <fieldset class="question ">
        <fieldset>
            <legend>
                <span class="question-label active" lang="">Registration place:</span>
                <span class="required">*</span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input name="/ANC_Registration_EngKan/reg_place" autocomplete="off"
                        data-name="/ANC_Registration_EngKan/reg_place" data-type-xml="select1"
                        required="required" type="radio" value="sub_center" />
                    <span class="option-label active" lang="">My Sub Center</span>
                </label>
                <label class="">
                    <input name="/ANC_Registration_EngKan/reg_place" autocomplete="off"
                        data-name="/ANC_Registration_EngKan/reg_place" data-type-xml="select1"
                        required="required" type="radio" value="phc" />
                    <span class="option-label active" lang="">My PHC</span>
                </label>
                <label class="">
                    <input name="/ANC_Registration_EngKan/reg_place" autocomplete="off"
                        data-name="/ANC_Registration_EngKan/reg_place" data-type-xml="select1"
                        required="required" type="radio" value="elsewhere" />
                    <span class="option-label active" lang="">Elsewhere
                    </span>
                </label>
            </div>
        </fieldset>
    </fieldset>

    <!--<label class="question non-select ">-->
    <!--<span lang="" class="question-label active">Thayi card number, -->
