    <!--</label>-->

    <label class="question non-select ">
        <span class="question-label active" lang="">ANC number</span>
        <span class="required">*</span>
        <input name="/ANC_Registration_EngKan/anc_number" autocomplete="off"
            data-constraint=".&gt; 0" data-type-xml="int" required="required" type="number" />
    </label>
    <!--<fieldset class="question ">-->
