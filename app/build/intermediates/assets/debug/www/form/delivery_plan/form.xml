            <legend>
                <span class="question-label active" lang="">Is there a family member who can join
                    this discussion?

                </span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input name="/Delivery_Plan_EngKan/family_member" autocomplete="off"
                        data-name="/Delivery_Plan_EngKan/family_member"
                        data-type-xml="select1" type="radio"
                        value="yes" />
                    <span class="option-label active" lang="">Yes</span>
                </label>
                <label class="">
                    <input name="/Delivery_Plan_EngKan/family_member" autocomplete="off"
                        data-name="/Delivery_Plan_EngKan/family_member"
                        data-type-xml="select1" type="radio"
                        value="no" />
                    <span class="option-label active" lang="">No</span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <fieldset class="question or-branch pre-init ">
        <fieldset>
            <legend>
                <span class="question-label active" lang="">The family plays an important role in
                    preparing for a safe
                    delivery. Is there a family member who can join this discussion?
                </span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input name="/Delivery_Plan_EngKan/family_member2" autocomplete="off"
                        data-name="/Delivery_Plan_EngKan/family_member2"
                        data-relevant=" /Delivery_Plan_EngKan/family_member  = 'no'" data-type-xml="select1"
                        type="radio"
                        value="yes" />
                    <span class="option-label active" lang="">Yes</span>
                </label>
                <label class="">
                    <input name="/Delivery_Plan_EngKan/family_member2" autocomplete="off"
                        data-name="/Delivery_Plan_EngKan/family_member2"
                        data-relevant=" /Delivery_Plan_EngKan/family_member  = 'no'" data-type-xml="select1"
                        type="radio"
                        value="no" />
                    <span class="option-label active" lang="">No</span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <fieldset class="question or-branch pre-init ">
        <fieldset>
            <legend>
                <span class="question-label active" lang="">Is contact number still,<span
                    class="or-output" data-value=" /Delivery_Plan_EngKan/entity_phone_no "></span>?
                </span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input name="/Delivery_Plan_EngKan/same_phone_no" autocomplete="off"
                        data-name="/Delivery_Plan_EngKan/same_phone_no"
                        data-relevant=" /Delivery_Plan_EngKan/entity_phone_no  != ''" data-type-xml="select1"
                        type="radio"
                        value="yes" />
                    <span class="option-label active" lang="">Yes</span>
                </label>
                <label class="">
                    <input name="/Delivery_Plan_EngKan/same_phone_no" autocomplete="off"
                        data-name="/Delivery_Plan_EngKan/same_phone_no"
                        data-relevant=" /Delivery_Plan_EngKan/entity_phone_no  != ''" data-type-xml="select1"
                        type="radio"
                        value="no" />
                    <span class="option-label active" lang="">No</span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <label class="question or-branch pre-init non-select ">
        <span class="question-label active" lang="">What is the new phone number?
        </span>
        <input name="/Delivery_Plan_EngKan/new_phone_no" autocomplete="off" data-constraint=".&gt; 999999999 and .&lt; 10000000000"
            data-relevant=" /Delivery_Plan_EngKan/same_phone_no  = 'no'"
            data-type-xml="int" type="number" />
    </label>
    <label class="question or-branch pre-init non-select ">
        <span class="question-label active" lang="">Contact phone number?
        </span>
        <span class="or-hint active" lang="">Without country code.
        </span><span class="required">*</span>
        <input name="/Delivery_Plan_EngKan/phone_no1" autocomplete="off" data-constraint=".&gt; 999999999 and .&lt; 10000000000"
            data-relevant=" /Delivery_Plan_EngKan/entity_phone_no  = ''"
            data-type-xml="int" type="number" />
    </label>
