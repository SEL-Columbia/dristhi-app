<form autocomplete="off" class="or clearfix" id="tambah_bayi" novalidate="novalidate">
    <!--This form was created by transforming a OpenRosa-flavored (X)Form using an XSLT sheet created by Enketo LLC.-->
    <section class="form-logo"></section>
    <h3 id="form-title">Tambah Bayi</h3>
    <select data-default-lang="Bahasa" id="form-languages">
        <option value="Bahasa">Bahasa</option>
        <option value="en">English</option>
    </select>

    <label class="question non-select ">
        <span class="question-label active" data-itext-id="/tambah_bayi_reviewed/no_bayi:label"
            lang="Bahasa">No. bayi
        </span>
        <span class="question-label " data-itext-id="/tambah_bayi_reviewed/no_bayi:label" lang="en">
            Baby's ID Number
        </span>
        <span class="or-hint active" data-itext-id="/tambah_bayi_reviewed/no_bayi:hint"
            lang="Bahasa">Diisi dengan nomor bayi
        </span>
        <span class="or-hint " data-itext-id="/tambah_bayi_reviewed/no_bayi:hint" lang="en">Fill in
            baby's number ID
        </span>
        <input name="/tambah_bayi_reviewed/no_bayi" data-type-xml="int" type="number" />
    </label>
    <label class="question non-select ">
        <span class="question-label active" data-itext-id="/tambah_bayi_reviewed/nama_bayi:label"
            lang="Bahasa">Nama bayi
        </span>
        <span class="question-label " data-itext-id="/tambah_bayi_reviewed/nama_bayi:label"
            lang="en">Name of baby
        </span>
        <span class="required">*</span>
        <span class="or-hint active" data-itext-id="/tambah_bayi_reviewed/nama_bayi:hint"
            lang="Bahasa">Apabila nama bayi belum ada, isi
            dengan: Bayi 'nama_ibu'. misal nama ibu Ratna, maka isi dengan Bayi Ratna
        </span>
        <span class="or-hint " data-itext-id="/tambah_bayi_reviewed/nama_bayi:hint" lang="en">If
            baby doesn't have a name yet, fill in: Baby "mother's name". For example, the mother's
            name is Ratna, then fill in Baby Ratna
        </span>
        <input name="/tambah_bayi_reviewed/nama_bayi" data-type-xml="string" required="required"
            type="text" />
    </label>
    <label class="question non-select ">
        <span class="or-constraint-msg active" data-itext-id="/tambah_bayi_reviewed/tanggal_lahir_anak:jr:constraintMsg"
            lang="Bahasa">Tanggal lahir
            bayi harus diisi dengan sebelum tanggal pendaftaran
        </span>
        <span class="or-constraint-msg " data-itext-id="/tambah_bayi_reviewed/tanggal_lahir_anak:jr:constraintMsg"
            lang="en">Date of baby's
            birth should be filled in with the date before registration date
        </span>
        <span class="question-label active" data-itext-id="/tambah_bayi_reviewed/tanggal_lahir_anak:label"
            lang="Bahasa">Tanggal lahir
        </span>
        <span class="question-label " data-itext-id="/tambah_bayi_reviewed/tanggal_lahir_anak:label"
            lang="en">DOB
        </span>
        <span class="required">*</span>
        <span class="or-hint active" data-itext-id="/tambah_bayi_reviewed/tanggal_lahir_anak:hint"
            lang="Bahasa">Diisi dengan tanggal lahir
            bayi
        </span>
        <span class="or-hint " data-itext-id="/tambah_bayi_reviewed/tanggal_lahir_anak:hint"
            lang="en">Fill in baby's birth of
            date
        </span>
        <input name="/tambah_bayi_reviewed/tanggal_lahir_anak" data-type-xml="date"
            required="required"
            type="date" />
    </label>
    <fieldset class="question ">
        <fieldset>
            <legend>
                <span class="question-label active" data-itext-id="/tambah_bayi_reviewed/jenis_kelamin:label"
                    lang="Bahasa">Jenis kelamin
                </span>
                <span class="question-label " data-itext-id="/tambah_bayi_reviewed/jenis_kelamin:label"
                    lang="en">Gender
                </span>
                <span class="required">*</span>
                <span class="or-hint active" data-itext-id="/tambah_bayi_reviewed/jenis_kelamin:hint"
                    lang="Bahasa">Diisi dengan jenis
                    kelamin bayi
                </span>
                <span class="or-hint " data-itext-id="/tambah_bayi_reviewed/jenis_kelamin:hint"
                    lang="en">Fill in baby's gender
                </span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input name="/tambah_bayi_reviewed/jenis_kelamin" data-name="/tambah_bayi_reviewed/jenis_kelamin"
                        data-type-xml="select1" required="required"
                        type="radio" value="laki" />
                    <span class="option-label active" data-itext-id="/tambah_bayi_reviewed/jenis_kelamin/laki:label"
                        lang="Bahasa">Laki-Laki (L)
                    </span>
                    <span class="option-label " data-itext-id="/tambah_bayi_reviewed/jenis_kelamin/laki:label"
                        lang="en">Male (L);
                    </span>
                </label>
                <label class="">
                    <input name="/tambah_bayi_reviewed/jenis_kelamin" data-name="/tambah_bayi_reviewed/jenis_kelamin"
                        data-type-xml="select1" required="required"
                        type="radio" value="perempuan" />
                    <span class="option-label active" data-itext-id="/tambah_bayi_reviewed/jenis_kelamin/perempuan:label"
                        lang="Bahasa">
                        Perempuan ( P)
                    </span>
                    <span class="option-label " data-itext-id="/tambah_bayi_reviewed/jenis_kelamin/perempuan:label"
                        lang="en">Female
                        (P)
                    </span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <fieldset class="question ">
        <fieldset>
            <legend>
                <span class="question-label active" data-itext-id="/tambah_bayi_reviewed/kepemilikan_buku_kia:label"
                    lang="Bahasa">Kepemilikan
                    buku KIA
                </span>
                <span class="question-label " data-itext-id="/tambah_bayi_reviewed/kepemilikan_buku_kia:label"
                    lang="en">Maternal and
                    Child Health booklet ownership (pink book)
                </span>
                <span class="or-hint active" data-itext-id="/tambah_bayi_reviewed/kepemilikan_buku_kia:hint"
                    lang="Bahasa">Diisi dengan
                    status kepemilikan buku KIA
                </span>
                <span class="or-hint " data-itext-id="/tambah_bayi_reviewed/kepemilikan_buku_kia:hint"
                    lang="en">Fill in
                    information whether mother has MCH Book
                </span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input name="/tambah_bayi_reviewed/kepemilikan_buku_kia" data-name="/tambah_bayi_reviewed/kepemilikan_buku_kia"
                        data-type-xml="select1" type="radio"
                        value="ya" />
                    <span class="option-label active" data-itext-id="/tambah_bayi_reviewed/kepemilikan_buku_kia/ya:label"
                        lang="Bahasa">Ya (+)
                    </span>
                    <span class="option-label " data-itext-id="/tambah_bayi_reviewed/kepemilikan_buku_kia/ya:label"
                        lang="en">Yes (+);
                    </span>
                </label>
                <label class="">
                    <input name="/tambah_bayi_reviewed/kepemilikan_buku_kia" data-name="/tambah_bayi_reviewed/kepemilikan_buku_kia"
                        data-type-xml="select1" type="radio"
                        value="tidak" />
                    <span class="option-label active" data-itext-id="/tambah_bayi_reviewed/kepemilikan_buku_kia/tidak:label"
                        lang="Bahasa">Tidak
                        (-)
                    </span>
                    <span class="option-label " data-itext-id="/tambah_bayi_reviewed/kepemilikan_buku_kia/tidak:label"
                        lang="en">No
                        (-)
                    </span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <label class="question non-select ">
        <span class="or-constraint-msg active" data-itext-id="/tambah_bayi_reviewed/berat_lahir:jr:constraintMsg"
            lang="Bahasa">Berat badan harus
            lebih dari 0
        </span>
        <span class="or-constraint-msg " data-itext-id="/tambah_bayi_reviewed/berat_lahir:jr:constraintMsg"
            lang="en">Birthweight should be
            more than 0
        </span>
        <span class="question-label active" data-itext-id="/tambah_bayi_reviewed/berat_lahir:label"
            lang="Bahasa">Berat lahir
        </span>
        <span class="question-label " data-itext-id="/tambah_bayi_reviewed/berat_lahir:label"
            lang="en">Baby's weight (gram)
        </span>
        <span class="or-hint active" data-itext-id="/tambah_bayi_reviewed/berat_lahir:hint"
            lang="Bahasa">Diisi dengan berat badan bayi
            saat lahir (gram)
        </span>
        <span class="or-hint " data-itext-id="/tambah_bayi_reviewed/berat_lahir:hint" lang="en">Fill
            in baby's birthweight (gram)
        </span>
        <input name="/tambah_bayi_reviewed/berat_lahir" data-constraint=".&gt;0 and .&lt;5000"
            data-type-xml="int" type="number" />
    </label>

    <fieldset style="display:none;" id="or-preload-items">
        <label class="calculation non-select ">
            <input name="/tambah_bayi_reviewed/start" data-preload="timestamp" data-preload-params="start"
                data-type-xml="dateTime" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/tambah_bayi_reviewed/today" data-preload="date" data-preload-params="today"
                data-type-xml="date" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/tambah_bayi_reviewed/deviceid" data-preload="property" data-preload-params="deviceid"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/tambah_bayi_reviewed/simserial" data-preload="property" data-preload-params="simserial"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/tambah_bayi_reviewed/phonenumber" data-preload="property" data-preload-params="phonenumber"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/tambah_bayi_reviewed/end" data-preload="timestamp" data-preload-params="end"
                data-type-xml="dateTime" type="hidden" />
        </label>
    </fieldset>
    <fieldset style="display:none;" id="or-calculated-items">
        <label class="calculation non-select ">
            <input name="/tambah_bayi_reviewed/meta/instanceID" data-calculate="concat('uuid:', uuid())"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/tambah_bayi_reviewed/formhub/uuid" data-calculate="'bfd243a85bea4e6e96dc1ff38497ed0c'"
                data-type-xml="string" type="hidden" />
        </label>
    </fieldset>
</form>